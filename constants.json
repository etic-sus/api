{
    "MODELS_NAME": {
        "Bots": "addbots",
        "Users": "User",
        "Feedbacks": "Feedbacks"
    },
    "PORT": 80,
    "ROUTES": {
        "BOTS": "/api/bots/:id(\\d{16,21})?(/:method(votes?|feedbacks))?",
